{
  "name": "taskbuyserver",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "rm -rf dist && ttsc && cp -R ./src/db/migrations ./dist/db",
    "dev": "cross-env DEVELOPMENT=true nodemon --watch src/** --ext ts --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "swagger": "tsoa spec",
    "mig": "cross-env DEVELOPMENT=true ts-node src/migration.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "bcrypt": "^5.0.1",
    "cluster": "^0.7.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-validator": "^6.14.0",
    "global": "^4.4.0",
    "jsonwebtoken": "^8.5.1",
    "node-postgres": "^0.6.2",
    "os": "^0.1.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "postgres-migrations": "^5.1.1",
    "prisma": "^4.7.1",
    "redis": "^4.5.0",
    "swagger-ui-express": "^4.6.0",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^15.0.2",
    "@types/pg": "^7.14.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/ws": "^8.5.3",
    "@types/yamljs": "^0.2.31",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tsoa": "^4.1.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "^2.2.3"
  }
}
